{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@api/*": [
        "src/api/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@config/*": [
        "src/config/*"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@redux/*": [
        "src/redux/*"
      ],
      "@routers/*": [
        "src/routers/*"
      ],
      "@services/*": [
        "src/services/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@templates/*": [
        "src/templates/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ]
    }
  }
}
